{
    "name": "symfony/demo-app",
    "type": "project",
    "require": {
        "php": "^8.1",
        "symfony/framework-bundle": "^6.4",
        "symfony/runtime": "^6.4",
        "symfony/yaml": "^6.4",
        "symfony/routing": "^6.4",
        "artprima/prometheus-metrics-bundle": "*"
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../..",
            "options": {
                "symlink": false
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}